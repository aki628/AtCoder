    {
      "name": "Launch read_seq_ch (cpptools)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/day1/read_seq_ch",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "miDebuggerPath": "/usr/bin/lldb",
      "setupCommands": [
        {
          "description": "Set lldb pretty printer settings",
